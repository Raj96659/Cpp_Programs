// Prime numbers are numbers greater then one that only have two factors, 1 and the number itself.

#include<iostream>
using namespace std;
main()
{
    int n,flag,i;
    cin>>n;

    flag=0;
    i=2;

    while(i<=n/2)
    {
        if(n%i==0)
        {
            flag=1;
            break;
        }
        i=i+1;
    }
    if(flag==0)
    {
        cout<<"\nPrime";
    }
    else
    {
        cout<<"\nNot prime";
    }
}
